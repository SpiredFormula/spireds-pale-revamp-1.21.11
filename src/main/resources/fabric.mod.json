	"authors": [
		"Spired"
	],
	"contact": {
		"homepage": "https://github.com/SpiredFormula/spireds-pale-revamp",
		"sources": "https://github.com/SpiredFormula/spireds-pale-revamp"
	},
	"license": "MIT",
	"icon": "assets/spireds-pale-revamp/icon.png",
